<!DOCTYPE html>
<html class="loading semi-dark-layout" lang="en" data-layout="semi-dark-layout" data-textdirection="ltr">
  <!-- BEGIN: Head-->
  <head>
    <title>
      <%= yield(:title) %>
    </title>

    <%= render 'shared/head' %>

    <link rel="stylesheet" type="text/css" href="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/css/core/menu/menu-types/vertical-menu.css">
  </head>
  <!-- END: Head-->

  <!-- BEGIN: Body-->

  <body class="vertical-layout vertical-menu-modern  navbar-floating footer-static" data-open="click" data-menu="vertical-menu-modern" data-col="">

    <%= render 'shared/site_javascript' %>

    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/jquery.dataTables.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/dataTables.bootstrap5.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/dataTables.responsive.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/responsive.bootstrap5.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/datatables.checkboxes.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/datatables.buttons.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/jszip.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/pdfmake.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/vfs_fonts.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/buttons.html5.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/buttons.print.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/tables/datatable/dataTables.rowGroup.min.js"></script>
    <script src="https://cdn.m6securities.com/vuexy_admin_7-3/app-assets/vendors/js/extensions/sweetalert2.all.min.js"></script>

    <!-- BEGIN: Header-->
    <%= render 'shared/app_navbar' %>
    <!-- END: Header-->

    <%= render 'shared/app_menu' %>

    <!-- BEGIN: Content-->
    <div class="app-content content">

      <div class="content-overlay"></div>
      <div class="header-navbar-shadow"></div>

      <div class="content-wrapper">

        <div id="page-alerts">
          <% # render "shared/app_alerts" %>
        </div>

        <div id="page-content">
          <% # render "shared/app_maintenance" %>
          <%= yield %>
        </div>

      </div>
    </div>
    <!-- END: Content-->

    <div class="sidenav-overlay"></div>
    <div class="drag-target"></div>

    <!-- BEGIN: Footer-->
    <%= render 'shared/app_footer' %>
    <!-- END: Footer-->

  </body>
  <!-- END: Body-->

</html>